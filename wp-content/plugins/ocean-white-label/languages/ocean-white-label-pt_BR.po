#: includes/themepanel/views/panes/white-label-settings.php:100
#: ocean-white-label.php:313
msgid "Preview Image"
msgstr "Pré-visualizar imagem"

#: includes/themepanel/views/panes/white-label-settings.php:76
#: includes/themepanel/views/panes/white-label-settings.php:104
#: ocean-white-label.php:317
msgid "Upload"
msgstr "Enviar"

